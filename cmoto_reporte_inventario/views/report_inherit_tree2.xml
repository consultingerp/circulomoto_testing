<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="reporte_inherited_tree2" model="ir.ui.view">
            <field name="name">reporte.inherited.tree2</field>
            <field name="model">stock.quant</field>
            <field name="inherit_id" ref="stock.view_stock_quant_tree"></field>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='quantity']" position='after'>
                    <field name="x_valor_promedio_related" string="Valor Promedio"/>
                    <field name="x_valor" string="Valor" />
                </xpath>
            </field>
        </record>
    </data>
</odoo>